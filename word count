n = int(input().strip())

counts = {}
order = []

for _ in range(n):
    word = input().strip()
    if word in counts:
        counts[word] += 1
    else:
        counts[word] = 1
        order.append(word)

# Output number of distinct words
print(len(order))

# Output occurrences in order of first appearance
print(" ".join(str(counts[word]) for word in order))
